#include <stdio.h>
int main() 
{
       int a, b, c;
       printf("Enter three numbers: ");
       scanf("%d %d %d", &a, &b, &c);
       int largest = a, smallest = a;
       if(b > largest) 
         {
            largest = b;
         }
        else if(c > largest)
         {
            largest = c;
         }
        else if(b < smallest)
         {
             smallest = b;
         }
        else
         {   
            smallest = c;
         }
 printf("Largest = %d, Smallest = %d\n", largest, smallest);
 return 0;
}
